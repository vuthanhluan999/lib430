;-----------------------------------------------------------------------------------------------
POPALL:         popx.a  R11
                popx.a  R10
                popx.a  R9
                popx.a  R8
                popx.a  R7
                popx.a  R6
                popx.a  R5
                popx.a  R4
                reta

;-----------------------------------------------------------------------------------------------
ZACwire:
;-----------------------------------------------------------------------------------------------
                mov.w   #BOR,R14
SearchStart:    cmp     #00A00h,0(R14)
                jhs     LongHold
                incd    R14
                cmp.w   #BOR+0160h,R14
                jne     SearchStart
SearchEnd:      ret

LongHold:       incd    R14
                cmp     #00200h,0(R14)
                jge     SearchStart
                cmp     #00120h,0(R14)
                jl      SearchStart
                incd    R14
                cmp     #00200h,0(R14)
                jge     SearchStart
                cmp     #00120h,0(R14)
                jl      SearchStart
StartBit:       clrc
                clr     R15
                mov     #016d,R6
NextBitH:       incd    R14
                cmp     #00200h,0(R14)
                jge     SetZeroH
SetOneH:        setc
                rlc     R15
                incd    R14
                dec     R6
                cmp     #08d,R6
                jne     NextBitH
                jmp     LowByte
                
SetZeroH:       clrc
                rlc     R15
                incd    R14
                dec     R6
                cmp     #08d,R6
                jne     NextBitH
                
LowByte:        incd    R14
                incd    R14
                incd    R14
                incd    R14
                
NextBitL:       incd    R14
                cmp     #00200h,0(R14)
                jge     SetZeroL
SetOneL:        setc
                rlc     R15
                incd    R14
                dec     R6
                cmp     #00d,R6
                jne     NextBitL
                jmp     SearchEnd
                
SetZeroL:       clrc
                rlc     R15
                incd    R14
                dec     R6
                cmp     #00d,R6
                jne     NextBitL
                
                jmp     SearchEnd

;-----------------------------------------------------------------------------------------------


;-----------------------------------------------------------------------------------------------
ConvertTSIC:	; Convert TSIC Value (R15) to Deg C (R8) using TempTabelle
		; Input:  R15, R7
		; Output: R8
;-----------------------------------------------------------------------------------------------
                clr     R7                      ; R7 Used as Pointer
                cmp     #0800h,R15
                jhs     TempError
                cmp     #0200h,R15
                jlo     NegativeTemp
                
PositiveTemp:   mov     #01500h,R12              ; Set Temperature Register to highest value (+150.0)
CMPRP:          cmp     TempTabelleP(R7),R15     ; Compare Table Value to TSIC
                jge     EndConvert              ; Jump if Sensor < Table
                incd    R7                      ; Increment word Pointer
                cmp     #0BB8h,R7
                jeq     TempError
                dadd    #09999h,R12              ; Subtract 1 from Counter
                jmp     CMPRP
                
NegativeTemp:   mov     #08000h,R12              ; Set Temperature Register to lowest value (0.0)
CMPRN:          cmp     TempTabelleN(R7),R15     ; Compare Table Value to TSIC
                jge     EndConvert              ; Jump if Sensor < Table
                incd    R7                      ; Increment word Pointer
                cmp     #03F0h,R7
                jeq     TempError
                dadd    #01h,R12                 ; Add 1 to Counter
                jmp     CMPRN

TempError       mov     #0EEEEh,R12

EndConvert:     ret                             ; Return from subroutine

