ZACTemp1
;-----------------------------------------------------------------------------------------------
                eint
                call	#PUSHALL
                bic.b   #VCC1,&P3OUT
                bic.b   #VSS,&P1OUT
                bis.b   #VSS,&P1DIR
                bis.b   #VCC1,&P3DIR
                bis.b   #TSIC1,&P1SEL
TimerA1         mov.w   #TASSEL_2+TACLR,&TA0CTL
                mov.w   #CM_3+CAP,&TA0CCTL0
            
                mov.w   #BOR,R14
                bis.w   #MC_2,&TA0CTL
                bis.b   #VCC1,&P3OUT
                bis.w   #CCIE,&TA0CCTL0
                
                bis.b   #LPM0,SR
                
                call    #ZACwire
                call    #ConvertTSIC
                call	#POPALL
                reta

